        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.21.4",
        "jest": "^26.5.3",
        "jest-css-modules": "^2.1.0",
        "postcss": "^8.4.31",
        "postcss-loader": "^6.2.1",
